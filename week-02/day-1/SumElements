using System;


namespace csprojects
{
    class MainClass
    {
        public static void Main(string[] args)
        {
            // - Create an array variable named `r`
            //   with the following content: `[54, 23, 66, 12]`
            // - Print the sum of the second and the third element
            int result;
            int[] r = { 54, 23, 66, 12 };
            result = r[1] + r[2];

            Console.WriteLine(result);

            Console.ReadLine();

        }
    }
}



