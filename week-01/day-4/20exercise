using System;

namespace csprojects
{
    class MainClass
    {
        public static void Main(string[] args)
        {
            {
                int number1;
                int number2;
                Console.Write("I'm going to decide which one is bigger. Give me a number: ");
                number1 = int.Parse(Console.ReadLine());

                Console.Write("Please also a second one: ");
                number2 = int.Parse(Console.ReadLine());

                if (number1>number2)
                {
                    Console.WriteLine(number1 +" is bigger I think... :)");
                }
                else
                {
                    Console.WriteLine(number2 +" is bigger you trikcy :)");
                }
            }
         }
     }
}
    


